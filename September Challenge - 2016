Link:- https://www.codechef.com/SEPT16/problems/LONGSEQ

Solve:-
#include <iostream>
#include <string.h>
using namespace std;

int main()
{
	int T,c1,c2; string d;
	cin>>T;
	while(T!=0)
	{
		T--;
		c1=0;c2=0;
		cin>>d;
		for(int i=0;i<d.length();i++)
		{	
			if(d[i]=='0')
			c1++;
			else
			c2++;
		}
		if(c2==1 || c1==1)
		cout<<"Yes"<<endl;
		else
		cout<<"No"<<endl;
	}
	return 0;
}
